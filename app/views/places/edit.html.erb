
<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Place</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br   />
    <div class="float-right">
      <input class="btn btn-primary" type="submit" value="Submit">
      <% f.submit 'Create', class: 'btn btn-primary' %>
    </div>
      <br />
  <% end %>
</div>
